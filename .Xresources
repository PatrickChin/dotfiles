Xft.dpi: 96
Xft.antialias: true
Xft.rgba: rgb
Xft.hinting: true
Xft.hintstyle: hintfull
Xft.autohint: 0
Xft.lcdfilter: lcddefault

URxvt*loginShell: true

URxvt*background: #0d1926
URxvt*foreground: #d9e6f2
URxvt*color0: #000000
URxvt*color8: #262626
URxvt*color1: #b87a7a
URxvt*color9: #dbbdbd
URxvt*color2: #7ab87a
URxvt*color10: #bddbbd
URxvt*color3: #b8b87a
URxvt*color11: #dbdbbd
URxvt*color4: #7a7ab8
URxvt*color12: #bdbddb
URxvt*color5: #b87ab8
URxvt*color13: #dbbddb
URxvt*color6: #7ab8b8
URxvt*color14: #bddbdb
URxvt*color7: #d9d9d9
URxvt*color15: #ffffff

URxvt*font: xft:Inconsolata\ for\ powerline:regular:pixelsize=17
URxvt*imFont: xft:Inconsolata\ for\ powerline:regular:pixelsize=17
URxvt*boldFont: xft:Inconsolata\ for\ powerline:regular:pixelsize=17

URxvt.perl-ext-common: default,clipboard,url-select,keyboard-select
URxvt.url-select.launcher: /usr/bin/xdg-open
URxvt.url-select.underline: true
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.keysym.M-s: perl:keyboard-select:search

URxvt*intensityStyles: false
URxvt*scrollstyle: plain
URxvt*scrollBar_right: true
URxvt*thickness: 10

URxvt*scrollTtyOutput: false
URxvt*scrollWithBuffer: true
URxvt*scrollTtyKeypress: true

URxvt*visualBell: false
URxvt.iso14755: false
URxvt.iso14755_52: false
